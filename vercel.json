{
  "version": 2,
  "functions": {
    "api/blog.js": { "includeFiles": "content/blog/**" },
    "api/blog-post.js": { "includeFiles": "content/blog/**" },
    "api/ssr/category.js": { "includeFiles": "category.html" },
    "api/ssr/country.js": { "includeFiles": "{united-states,canada,india,japan}.html" }
  },
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/wishlist", "destination": "/wishlist.html" },
    { "source": "/wishlist/", "destination": "/wishlist.html" },
    { "source": "/categories", "destination": "/categories.html" },
    { "source": "/categories/", "destination": "/categories.html" },
    { "source": "/categories/:slug", "destination": "/api/ssr/category?slug=:slug" },
    { "source": "/categories/:slug/", "destination": "/api/ssr/category?slug=:slug" },
    { "source": "/locations", "destination": "/locations.html" },
    { "source": "/locations/", "destination": "/locations.html" },
    { "source": "/country/united-states", "destination": "/api/ssr/country?name=united-states" },
    { "source": "/country/united-states/", "destination": "/api/ssr/country?name=united-states" },
    { "source": "/country/canada", "destination": "/api/ssr/country?name=canada" },
    { "source": "/country/canada/", "destination": "/api/ssr/country?name=canada" },
    { "source": "/country/india", "destination": "/api/ssr/country?name=india" },
    { "source": "/country/india/", "destination": "/api/ssr/country?name=india" },
    { "source": "/country/japan", "destination": "/api/ssr/country?name=japan" },
    { "source": "/country/japan/", "destination": "/api/ssr/country?name=japan" },
    { "source": "/near-me", "destination": "/near-me.html" },
    { "source": "/near-me/", "destination": "/near-me.html" },
    
    { "source": "/es", "destination": "/es/index.html" },
    { "source": "/es/", "destination": "/es/index.html" },
    { "source": "/es/categories", "destination": "/es/categories.html" },
    { "source": "/es/categories/", "destination": "/es/categories.html" },
    { "source": "/es/categories/:slug", "destination": "/es/category.html" },
    { "source": "/es/categories/:slug/", "destination": "/es/category.html" },
    { "source": "/es/locations", "destination": "/es/locations.html" },
    { "source": "/es/locations/", "destination": "/es/locations.html" },
    { "source": "/es/country/united-states", "destination": "/es/united-states.html" },
    { "source": "/es/country/united-states/", "destination": "/es/united-states.html" },
    { "source": "/es/country/canada", "destination": "/es/canada.html" },
    { "source": "/es/country/canada/", "destination": "/es/canada.html" },
    { "source": "/es/country/india", "destination": "/es/india.html" },
    { "source": "/es/country/india/", "destination": "/es/india.html" },
    { "source": "/es/country/japan", "destination": "/es/japan.html" },
    { "source": "/es/country/japan/", "destination": "/es/japan.html" },
    { "source": "/es/near-me", "destination": "/es/near-me.html" },
    { "source": "/es/near-me/", "destination": "/es/near-me.html" },

    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/blog/", "destination": "/blog.html" },
    { "source": "/es/blog", "destination": "/blog.html" },
    { "source": "/es/blog/", "destination": "/blog.html" },

    { "source": "/admin", "destination": "/admin/index.html" },
    { "source": "/admin/", "destination": "/admin/index.html" },

    { "source": "/blog/:slug", "destination": "/blog-post.html" },
    { "source": "/blog/:slug/", "destination": "/blog-post.html" },
    { "source": "/es/blog/:slug", "destination": "/blog-post.html" },
    { "source": "/es/blog/:slug/", "destination": "/blog-post.html" }
  ],
  "redirects": [
    { "source": "/categories/indian", "destination": "/country/india/", "statusCode": 301 },
    { "source": "/categories/indian/", "destination": "/country/india/", "statusCode": 301 },
    { "source": "/categories/japanese", "destination": "/country/japan/", "statusCode": 301 },
    { "source": "/categories/korean", "destination": "/categories", "statusCode": 301 },
    { "source": "/categories/scottish", "destination": "/categories", "statusCode": 301 },
    { "source": "/categories/asian", "destination": "/categories", "statusCode": 301 },

    { "source": "/index.html", "destination": "/", "statusCode": 301 },
    { "source": "/categories.html", "destination": "/categories", "statusCode": 301 },
    { "source": "/category.html", "destination": "/categories", "statusCode": 301 },
    { "source": "/locations.html", "destination": "/locations", "statusCode": 301 },
    { "source": "/near-me.html", "destination": "/near-me", "statusCode": 301 },
    { "source": "/wishlist.html", "destination": "/wishlist", "statusCode": 301 },
    { "source": "/blog.html", "destination": "/blog", "statusCode": 301 },
    { "source": "/creator.html", "destination": "/", "statusCode": 301 },
    { "source": "/canada.html", "destination": "/country/canada", "statusCode": 301 },
    { "source": "/india.html", "destination": "/country/india", "statusCode": 301 },
    { "source": "/japan.html", "destination": "/country/japan", "statusCode": 301 },
    { "source": "/united-states.html", "destination": "/country/united-states", "statusCode": 301 },

    { "source": "/es/index.html", "destination": "/es/", "statusCode": 301 },
    { "source": "/es/blog.html", "destination": "/es/blog", "statusCode": 301 },
    { "source": "/es/categories.html", "destination": "/es/categories", "statusCode": 301 },
    { "source": "/es/category.html", "destination": "/es/categories", "statusCode": 301 },
    { "source": "/es/locations.html", "destination": "/es/locations", "statusCode": 301 },
    { "source": "/es/near-me.html", "destination": "/es/near-me", "statusCode": 301 },
    { "source": "/es/wishlist.html", "destination": "/es/wishlist", "statusCode": 301 },
    { "source": "/es/creator.html", "destination": "/es/", "statusCode": 301 },
    { "source": "/es/canada.html", "destination": "/es/country/canada", "statusCode": 301 },
    { "source": "/es/india.html", "destination": "/es/country/india", "statusCode": 301 },
    { "source": "/es/japan.html", "destination": "/es/country/japan", "statusCode": 301 },
    { "source": "/es/united-states.html", "destination": "/es/country/united-states", "statusCode": 301 }
  ],
  "headers": [
    {
      "source": "/sitemap(.*)\\.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/categories.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/category.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/united-states.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/locations.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/wishlist.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/creator.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/config/categories.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ]
}